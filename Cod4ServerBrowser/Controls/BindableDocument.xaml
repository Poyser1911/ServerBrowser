<UserControl x:Class="Cod4ServerBrowser.Controls.BindableDocument"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Cod4ServerBrowser.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <RichTextBox x:Name="RichTextBox" Background="{x:Null}" BorderBrush="{x:Null}" BorderThickness="0" IsReadOnly="True" Cursor="Hand">
        <FlowDocument/>
    </RichTextBox>
</UserControl>
